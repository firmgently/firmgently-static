{% extends "fg-template.njk" %}


{% block main %}
  <h2>Pages in the <strong>{{ tag }}</strong> category</h2>
  <ul>
    {% for page in pagination.files %}
    <li>
    <a  title="{{ page.title }}" href="/{{ page.path }}">{{ page.title }}</a>
    {{ page.excerpt | safe }}
    </li>
  {% endfor %}
  </ul>
{% endblock %}
